<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="334.0" prefWidth="478.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.progtechbeadando.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane id="Container" prefHeight="306.0" prefWidth="438.0">
      <children>
         <Label layoutX="17.0" layoutY="6.0" text="Neptunkód:" />
         <TextField fx:id="LoginNeptunBox" layoutX="19.0" layoutY="34.0" prefHeight="25.0" prefWidth="407.0" />
         <Label layoutX="19.0" layoutY="74.0" prefHeight="17.0" prefWidth="37.0" text="Jelszó:" />
          <Button layoutX="69.0" layoutY="161.0" onAction="#onLoginButtonClick" prefHeight="25.0" prefWidth="300.0" text="Bejelentkezés" />
         <Button layoutX="179.0" layoutY="218.0" mnemonicParsing="false" onAction="#onRegButtonClick" text="Regisztráció" />
         <PasswordField fx:id="LoginJelszoBox" layoutX="17.0" layoutY="116.0" prefHeight="25.0" prefWidth="407.0" />
      </children>
   </AnchorPane>

    <Label fx:id="ErrorLogin" text="Jelentkezzen be, vagy regisztráljon a folytatáshoz!"/>
</VBox>
